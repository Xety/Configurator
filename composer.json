{
    "name": "xety/configurator",
    "description": "A simple configuration class without dependencies that use the Fluent pattern.",
    "keywords": [
        "configuration",
        "fluent",
        "config"
    ],
    "homepage": "https://github.com/Xety/Configurator",
    "support": {
        "source": "https://github.com/Xety/Configurator",
        "issues": "https://github.com/Xety/Configurator/issues"
    },
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Xety",
            "email": "emeric@xeta.io",
            "homepage": "https://github.com/Xety"
        }
    ],
    "autoload": {
        "psr-4": {
            "Xety\\Configurator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xety\\Configurator\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "squizlabs/php_codesniffer": "~3.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
